# Puppet Managed
Host aiadm.cern.ch, aiadm8.cern.ch, aiadm8-homeless.cern.ch, lxtunnel.cern.ch
  User straylen
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlMaster auto
  ControlPersist 10m
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  DynamicForward 8080
  ExitOnForwardFailure no
  GSSAPITrustDns yes
  CheckHostIP no

Host *.cern.ch !aiadm !aiadm.cern.ch !gitlab.cern.ch !aiadm8 !aiadm8.cern.ch !aiadm8-homeless.cern.ch !lxtunnel.cern.ch
   User root
   StrictHostKeyChecking no
   ProxyJump aiadm8-homeless.cern.ch
   CheckHostIP no

Host *
  User straylen
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlMaster auto
  ControlPersist 10m
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  GSSAPITrustDns yes
  CheckHostIP no


