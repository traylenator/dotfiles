# Puppet Managed
Host aiadm.cern.ch, aiadm8.cern.ch, aiadm8-homeless.cern.ch, lxtunnel.cern.ch
  User straylen
  #ControlPath /run/user/%i/%r@%h:%p
  #ControlMaster auto
  #ControlPersist 10m
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  DynamicForward 8080
  ExitOnForwardFailure no
  GSSAPITrustDns yes
  PubkeyAuthentication no
  CheckHostIP no


Host *.cern.ch !aiadm !aiadm.cern.ch !gitlab.cern.ch !aiadm8 !aiadm8.cern.ch !aiadm8-homeless.cern.ch !lxtunnel.cern.ch
   User straylen
   StrictHostKeyChecking no
   #ProxyJump lxtunnel.cern.ch
   CheckHostIP no

Host *
  User straylen
  ControlPath /run/user/%i/%r@%h:%p
  ControlMaster auto
  ControlPersist 10m
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  GSSAPITrustDns yes
  CheckHostIP no

